pipeline {
  agent any
  stages {
    stage('Launch FrontEnd Pipeline') {
      when {
        changeset "EmployeeProjectFront/*"
      }
      steps {
        build job: 'FrontEmployeePipeline', parameters: []
      }
    }
    stage('Launch BackEnd Pipeline') {
      when {
        changeset "EmployeeProjectBack/*"
      }
      steps {
        build job: 'BackEmployeePipeline', parameters: []
      }
    }
  }
}







